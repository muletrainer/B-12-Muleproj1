<?xml version='1.0' encoding='UTF-8'?>
<types:mule xmlns:types="http://www.mulesoft.org/schema/mule/types">
  <types:catalog>
    <types:type name="students-csv" format="json">
      <types:shape format="weave" example="examples/Student.json"><![CDATA[%dw 2.0

type students_csv = {|
  sno: String, 
  sname: String, 
  m1: String, 
  m2: String
|} {"example": "{\\\"sno\\\":\\\"1\\\",\\\"sname\\\":\\\"raju\\\",\\\"m1\\\":\\\"10\\\",\\\"m2\\\":\\\"20\\\"}"}

]]></types:shape>
    </types:type>
    <types:type name="stud-csv" format="csv">
      <types:shape format="weave" example="examples/students-out.csv"><![CDATA[%dw 2.0

type stud_csv = Array<{|  sno: String,   sname: String,   totMarks: String|}> {"isQuotedString": "false",
"header": "true",
"headerLineNumber": "0",
"separator": ","}

]]></types:shape>
      <types:properties>
        <types:property name="headerLineNumber" value="0"/>
        <types:property name="header" value="true"/>
        <types:property name="isQuotedString" value="false"/>
        <types:property name="separator" value=","/>
      </types:properties>
    </types:type>
  </types:catalog>
  <types:enrichment select="#4b7994c9-70bf-4f31-a1aa-24157807bbaa">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="students-csv"/>
        </types:message>
      </types:input-event>
      <types:output-event>
        <types:message>
          <types:payload type="stud-csv"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
</types:mule>